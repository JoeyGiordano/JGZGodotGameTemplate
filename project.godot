; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="JGZGodotGameTemplate"
run/main_scene="res://scenes/game_container/game_container.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

AudioManager="*res://autoloads/AudioManager.gd"
Debug="*res://autoloads/Debug.gd"
GameInfo="*res://autoloads/GameInfo.gd"
GameManager="*res://autoloads/GameManager.gd"
Global="*res://autoloads/Global.gd"
LevelLoader="*res://autoloads/LevelLoader.gd"
ShellSceneManager="*res://autoloads/ShellSceneManager.gd"
Utility="*res://autoloads/Utility.gd"
Ref="*res://autoloads/Reference/Reference.gd"

[display]

window/size/mode=2
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/plugin_restarter/plugin.cfg", "res://addons/reference_book/plugin.cfg")

[input]

DEBUG_QUIT={
"deadzone": 0.2,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":4194305,"key_label":0,"unicode":0,"location":0,"echo":false,"script":null)
]
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
